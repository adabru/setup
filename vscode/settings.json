{
  "editor.tabSize": 2,
  "window.enableMenuBarMnemonics": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "boundary",
  "files.trimTrailingWhitespace": true,
  "editor.rulers": [100],
  "editor.tokenColorCustomizations": {
    "[Default Dark+]": {
      "textMateRules": [
        { "scope": ["source.adabrumarkup", "toc"], "settings": { "foreground": "#a63" } },
        { "scope": ["source.adabrumarkup", "unimportant"], "settings": { "foreground": "#444444" } },
        { "scope": ["source.adabrumarkup", "heading.l1"], "settings": { "foreground": "#E1993A" } },
        { "scope": ["source.adabrumarkup", "heading.l2"], "settings": { "foreground": "#C3A743" } },
        { "scope": ["source.adabrumarkup", "heading.l3"], "settings": { "foreground": "#A5B34E" } },
        { "scope": ["source.adabrumarkup", "heading.l4"], "settings": { "foreground": "#53C146" } },
        { "scope": ["source.adabrumarkup", "heading.l4"], "settings": { "foreground": "#53C146" } },
        { "scope": ["source.adabrumarkup", "info"], "settings": { "foreground": "#540" } },
        { "scope": ["source.adabrumarkup", "warn"], "settings": { "foreground": "#922" } },
        { "scope": ["source.adabrumarkup", "listitem"], "settings": { "foreground": "#222" } },
        { "scope": ["source.adabrumarkup", "quoted"], "settings": { "foreground": "#bdcbfc" } },
        { "scope": ["source.adabrumarkup", "emphasize"], "settings": { "foreground": "#ffffff" } },
        { "scope": ["source.adabrumarkup", "bold"], "settings": { "fontStyle": "bold" } },
        { "scope": ["source.adabrumarkup", "file"], "settings": { "foreground": "#bfb" } },
        { "scope": ["source.adabrumarkup", "folder"], "settings": { "foreground": "#34e2e1" } },
        { "scope": ["source.adabrumarkup", "time"], "settings": { "foreground": "#bb4444" } },
        { "scope": ["source.adabrumarkup", "bracket"], "settings": { "foreground": "#842" } },
        { "scope": ["source.adabrumarkup", "link"], "settings": { "foreground": "#bbbbff" } },
        { "scope": ["source.adabrumarkup", "terminal"], "settings": { "foreground": "#ad36b8" } },
        { "scope": ["source.adabrumarkup", "tableeven"], "settings": { "background": "#300a24" } },
        { "scope": ["source.adabrumarkup", "tableodd"], "settings": { "background": "#333" } },
        { "scope": ["source.adabrumarkup", "keystroke"], "settings": { "foreground": "#ac4cfa" } },
        { "scope": ["source.adabrumarkup"], "settings": { "foreground": "#ddd", "fontStyle": "" } }
      ]
    }
  },
  "keyboard.dispatch": "keyCode",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "arduino.additionalUrls": [
    "http://drazzy.com/package_drazzy.com_index.json",
    "https://dl.espressif.com/dl/package_esp32_index.json"
  ],
  "mesonbuild.configureOnOpen": true,
  "latex-workshop.view.pdf.viewer": "tab",
  "C_Cpp.updateChannel": "Insiders",
  "[sql]": {},
  "editor.selectionClipboard": false,
  "python.jediEnabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "showoffset.offsetType": "byte",
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "update.mode": "none",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnType": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.printWidth": 120,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "[cpp]": {
    "editor.defaultFormatter": "chiehyu.vscode-astyle"
  },
  "git.countBadge": "off",
  "git.enableStatusBarSync": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "python.languageServer": "Pylance",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.tabSize": 4
  },
  "astyle.astylerc": "/usr/share/arduino/lib/formatter.conf",
  "dart.debugExternalLibraries": true,
  "dart.debugSdkLibraries": true,
  "files.exclude": {
    "**/node_modules": true
  },
  "python.formatting.provider": "black",
  "window.zoomLevel": -1,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "cursorless.hatSizeAdjustment": -30,
  "python.analysis.extraPaths": [
    "/home/adabru/downloads/talon-linux-109-0.2.0-357-g3b3e/talon/resources/python/lib/python3.9/site-packages/"
  ],
  "git.confirmSync": false
}
